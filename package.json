{
  "name": "@pcmnac/react-wizard-bootstrap3-renderer",
  "description": "Bootstrap 3 renderer for @pcmnac/react-wizard",
  "author": "Paulo Coutinho",
  "user": "pcmnac",
  "version": "0.0.7",
  "scripts": {
    "start": "catalog start docs",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:lint": "eslint . --ignore-path .gitignore",
    "gh-pages": "catalog build docs",
    "gh-pages:deploy": "gh-pages -d docs/build",
    "dist:es6": "del-cli ./dist-es6 && cross-env BABEL_ENV=es6 babel ./src --out-dir ./dist-es6",
    "dist:modules": "del-cli ./dist-modules && cross-env BABEL_ENV=modules babel ./src --out-dir ./dist-modules",
    "preversion": "npm run test",
    "prepublishOnly": "npm run dist:es6 && npm run dist:modules",
    "postpublish": "npm run gh-pages && npm run gh-pages:deploy",
    "postinstall": "node lib/post_install.js"
  },
  "main": "dist-modules",
  "module": "dist-es6",
  "jsnext:main": "dist-es6",
  "devDependencies": {
    "@pcmnac/react-wizard": "^0.0.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.9",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "catalog": "^3.0.0",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "del-cli": "^1.1.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.2",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^1.0.0",
    "git-prepush-hook": "^1.0.2",
    "jest": "^21.2.1",
    "lodash": "^4.17.4",
    "purecss": "^1.0.0",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^16.0.0",
    "react-github-corner": "^1.0.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.4.3",
    "sync-exec": "^0.6.2",
    "webpack": "^3.6.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.4",
    "react": ">= 0.11.2 < 16.0.0",
    "react-bootstrap": "0.31.3",
    "rxjs": "^5.4.3",
    "@pcmnac/react-wizard": "^0.0.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pcmnac/react-wizard-bootstrap3-renderer.git"
  },
  "homepage": "https://github.com/pcmnac/react-wizard-bootstrap3-renderer/wiki",
  "bugs": {
    "url": "https://github.com/pcmnac/react-wizard-bootstrap3-renderer"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ]
  },
  "keywords": [
    "react",
    "reactjs",
    "wizard",
    "bootstrap"
  ],
  "license": "MIT",
  "pre-push": [
    "test"
  ]
}
